@article{yuanFasterLightDetection2023,
  title = {Faster {{Light Detection Algorithm}} of {{Traffic Signs Based}} on {{YOLOv5s-A2}}},
  author = {Yuan, Xu and Kuerban, Alifu and Chen, Yixiao and Lin, Wenlong},
