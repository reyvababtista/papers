@inproceedings{phanUnderstandingHeavyDrinking2020,
  title = {Understanding {{Heavy Drinking}} at {{Night}} through {{Smartphone Sensing}} and {{Active Human Engagement}}},
  booktitle = {Proceedings of the 14th {{EAI International Conference}} on {{Pervasive Computing Technologies}} for {{Healthcare}}},
